{
  "actorSpecification": 1,
  "title": "SBA Certifications Search Results",
  "description": "Structured output view for results returned from https://search.certifications.sba.gov/_api/v2/search.",
  "views": {
    "defaultTable": {
      "title": "Businesses",
      "description": "Key details for each business returned by the SBA certifications search API.",
      "transformation": {
        "fields": [
          "entityDetailId",
          "businessName",
          "uei",
          "cageCode",
          "location.state",
          "location.city",
          "naicsPrimary",
          "sbaCertifications",
          "website",
          "lastUpdateDateIso"
        ]
      },
      "display": {
        "component": "table",
        "properties": {
          "entityDetailId": {
            "label": "Entity ID"
          },
          "businessName": {
            "label": "Business"
          },
          "uei": {
            "label": "UEI"
          },
          "cageCode": {
            "label": "CAGE"
          },
          "location.state": {
            "label": "State"
          },
          "location.city": {
            "label": "City"
          },
          "naicsPrimary": {
            "label": "Primary NAICS"
          },
          "sbaCertifications": {
            "label": "SBA Certifications"
          },
          "website": {
            "label": "Website",
            "format": "link"
          },
          "lastUpdateDateIso": {
            "label": "Updated"
          }
        }
      }
    }
  }
}
